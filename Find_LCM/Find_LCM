#include <bits/stdc++.h>
using namespace std;

void main()
{
    int t, n, max;

    cout << "Enter two numbers to find LCM: ";
    cin >> t >> n;
    
    // maximum value between t and n is stored in max
    max = (t > n) ? t : n;

    do
    {
        if (max % t == 0 && max % n == 0)
        {
            cout << "LCM = " << max;
            break;
        }
        else
            ++max;
    } while (1);
}
